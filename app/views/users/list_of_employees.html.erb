<% provide(:title, "出勤中社員一覧") %>
<h1>出勤中社員一覧</h1>

<div class="col-md-10 col-md-offset-1">
  <table class="table table-condensed table-hover" id="table-users">
    <thead>
      <tr>
        <th>社員番号</th>
        <th>氏名</th>
      </tr>
    </thead>
     <tbody>
      <% @in_attendances.each do |in_attendance| %>
        <tr>
          <td><%= in_attendance.user.employee_number %></td>
          <td><%= in_attendance.user.name %></td>
        </tr>
      <% end %>
     </tbody>
  </table>
</div>


